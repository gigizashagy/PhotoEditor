QTabBar QToolButton::right-arrow:enabled {
    image: url(:/stylesheets/editor_style_sheet/rc/right_arrow.png);
    background-color: #3d3e40;
}

QTabBar QToolButton::left-arrow:enabled {
    image: url(:/stylesheets/editor_style_sheet/rc/left_arrow.png);
    background-color: #3d3e40;
}

QTabBar QToolButton::right-arrow:disabled {
    image: url(:/stylesheets/editor_style_sheet/rc/right_arrow_disabled.png);
    background-color: #3d3e40;
 }

QTabBar QToolButton::left-arrow:disabled {
    image: url(:/stylesheets/editor_style_sheet/rc/left_arrow_disabled.png);
    background-color: #3d3e40;
}


QDockWidget {
    border: 1px solid #403F3F;
    titlebar-close-icon: url(:/stylesheets/editor_style_sheet/rc/close.png);
    titlebar-normal-icon: url(:/stylesheets/editor_style_sheet/rc/undock.png);
}

QDockWidget::close-button, QDockWidget::float-button {
    border: 1px solid transparent;
    border-radius: 2px;
    background: transparent; 
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: rgba(255, 255, 255, 10);
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
    background: rgba(255, 255, 255, 10);
}

QTreeView
{
    border: 1px solid #515255;
    background-color: #292a2b;
    alternate-background-color: #222324;
    color: #E4E4E4;
    paint-alternating-row-colors-for-empty-area: 1;
    min-height: 70px;
}

QListView
{
    border: 1px solid #515255;
    background-color: #292a2b;
    color: #E4E4E4;
    min-height: 70px;
}

QComboBox QListView {
    min-height: 0px;
}

QTreeView:branch:selected, QTreeView:branch:selected:hover, QTreeView:branch:selected:disabled
{
    background: #396280;
}

QTreeView:branch:hover
{
    background: url(:/stylesheets/editor_style_sheet/rc/transparent.png);
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(:/stylesheets/editor_style_sheet/rc/transparent.png);
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(:/stylesheets/editor_style_sheet/rc/transparent.png);
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(:/stylesheets/editor_style_sheet/rc/transparent.png);
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    image: url(:/stylesheets/editor_style_sheet/rc/branch_closed.png);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
    image: url(:/stylesheets/editor_style_sheet/rc/branch_open.png);
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover {
    image: url(:/stylesheets/editor_style_sheet/rc/branch_closed-on.png);
}

QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover  {
    image: url(:/stylesheets/editor_style_sheet/rc/branch_open-on.png);
}

QTreeView QLineEdit {
    padding: 0px;
    margin-left: 1px;
    min-height: 11px;
    width: 100%;
    border: none;
}

QListView::item:!selected:hover, QTreeView::item:!selected:hover {
    background: rgba(0,0,0,0);
    outline: 0;
    color: #f5f5f5;
}

QListView::item:!selected:disabled, QTreeView::item:!selected:disabled {
    background-color: rgba(0,0,0,0);
    color: #646464;
}

QListView::item:selected:hover, QTreeView::item:selected:hover, QListView::item:selected:disabled, QTreeView::item:selected:disabled {
    background: #396280;
}

QSlider {
    background: transparent;
    height: 24px;
}

QSlider::groove:horizontal {
    border: 0px solid #3A3939;
    height: 6px;
    background: #585A5E;
    margin: 1px 0;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: white;
    width: 16px;
    margin: -5px 0;
    border-radius: 8px;
}

QSlider::groove:vertical {
    border: 1px solid #3A3939;
    width: 8px;
    background: #201F1F;
    margin: 0 0px;
    border-radius: 2px;
}

QSlider::handle:vertical {
    background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.0 silver,
      stop: 0.2 #a8a8a8, stop: 1 #727272);
    border: 1px solid #3A3939;
    width: 10px;
    height: 10px;
    margin: 0 -4px;
    border-radius: 2px;
}

QToolButton {
    background-color: transparent;
    border: 0px solid transparent;
    border-radius: 2px;
    min-width: 17px;
    min-height: 17px;
    margin-right: 2px;
    margin-top: 0px;
    margin-bottom: 0px;
    padding: 0px;
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 18px; /* make way for the popup button */
    /*border-radius: 2px;*/
    /*border: 1px solid #4A4949;*/
}

QToolButton[popupMode="2"] { /* only for InstantPopup */
    padding-right: 10px; /* make way for the popup button */
    /*border: 1px solid #4A4949;*/
}


QToolButton:hover, QToolButton::menu-button:hover {
    /*background-color: transparent;
    border: 1px solid #78879b;*/
    background-color: #5a5b5e;
}


QToolButton:checked, QToolButton:pressed, QToolButton::menu-button:checked, QToolButton::menu-button:pressed {
    /*background-color: #4A4949;*/
    background-color: #292a2b;
}


/* the subcontrol below is used only in the InstantPopup or DelayedPopup mode */
QToolButton::menu-indicator {
    image: url(:/stylesheets/editor_style_sheet/rc/down_arrow.png);
    subcontrol-origin: padding;
    subcontrol-position: center right;
    right: 2px;
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 1px transparent #4A4949;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
    outline: none;
}

QToolButton::menu-arrow {
    image: url(:/stylesheets/editor_style_sheet/rc/down_arrow.png);
}

QTableView
{
    border: 1px solid #444;
    gridline-color: #6c6c6c;
    background-color: #201F1F;
}


QTableView, QHeaderView
{
    border-radius: 0px;
}

QTableView::item:pressed, QListView::item:pressed, QTreeView::item:pressed  {
    color: #f5f5f5;
}

QTableView::item:selected:active, QTreeView::item:selected:active, QListView::item:selected:active  {
    color: #f5f5f5;
}


QHeaderView
{
    border: 1px transparent;
    border-radius: 2px;
    margin: 0px;
    padding: 0px;
    background-color: #353638;
}

QHeaderView::section  {
    background-color: #353638;
    color: silver;
    padding: 4px;
    border: 1px solid #6c6c6c;
    border-radius: 0px;
    text-align: center;
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one
{
    border-top: 1px solid #6c6c6c;
}

QHeaderView::section::vertical
{
    border-top: transparent;
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one
{
    border-left: 1px solid #6c6c6c;
}

QHeaderView::section::horizontal
{
    border-left: transparent;
}


QHeaderView::section:checked
 {
    color: #f5f5f5;
    background-color: #5A5959;
 }

 /* style the sort indicator */
QHeaderView::down-arrow {
    image: url(:/stylesheets/editor_style_sheet/rc/down_arrow.png);
}

QHeaderView::up-arrow {
    image: url(:/stylesheets/editor_style_sheet/rc/up_arrow.png);
}


QTableCornerButton::section {
    background-color: #3A3939;
    border: 1px solid #3A3939;
    border-radius: 2px;
}

QToolBox  {
    padding: 0px;
    border: 1px solid black;
}

QToolBox::tab {
    color: #b1b1b1;
    background-color: #302F2F;
    border: 1px solid #4A4949;
    border-bottom: 1px transparent #302F2F;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

QToolBox::tab:selected { /* italicize selected tabs */
    font: italic;
    background-color: #302F2F;
    border-color: #3d8ec9;
}

QStatusBar
{
    border: 0px solid #292A2C;
    min-height: 64px;
}

QStatusBar::item {
    border: 0px solid #3A3939;
    border-radius: 2px;
}

QSplitter {
    background-color: transparent;
}

QSplitter::handle {
    background-color: transparent;
}

QSplitter::handle:hover {
    background-color: #787876;
    border: 2px solid #787876;
}
