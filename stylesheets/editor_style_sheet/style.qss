    background: transparent;
    selection-background-color: #0E567B;
    padding: 0px 3px;
    border: 1px solid #3C3D3F;
    border-radius: 4px;
    color: #C2C5CC;
    line-height: normal;
}

QTextEdit
{
    background-color: #292a2b;
    selection-background-color: #0E567B;
    color: #C2C5CC;
    border: 1px solid #515255;
    border-radius: 0px;
    padding: 0px 3px;
}

QPlainTextEdit
{
    background-color: #292a2b;
    selection-background-color: #0E567B;
    color: #C2C5CC;
    border: 1px solid #515255;
