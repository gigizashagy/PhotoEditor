QWidget
{
    color: #C2C5CC;
    background-color: #1D1D1E;
    selection-background-color: #396280;
    selection-color: #f5f5f5;
    font-family: "Roboto";
    font-size: 14px;
}

QWidget:item:hover
{
    background-color: #293640;
}

QWidget:item:selected
{
    background-color: #396280;
}

QWidget:item:disabled
{
    background-color: #414245;
    color: #646464;
}

QWidget:disabled
{
    color: #646464;
}

MainWindow
{
    background-color: #808080;
}

QMenuBar:focus
{
    border: 0px solid #292A2B;
}

/* SCROLL */
QScrollBar:horizontal, QScrollBar:vertical
{
    width: 12px;
    height: 12px;
    border-radius: 4px;
    border: 2px solid #2A2929;

}

QScrollBar::handle:horizontal, QScrollBar::handle:vertical
{
    border-radius: 4px;
    background-color: #555659;
    border: 2px solid #555659;
    min-height: 10px;
    min-width: 10px;
}

QScrollBar::handle:horizontal:hover, QScrollBar::handle:horizontal:active,
QScrollBar::handle:vertical:hover, QScrollBar::handle:vertical:active
{
    background-color: #616366;
    border: 2px solid #616366;
}

QScrollBar::sub-line:horizontal, QScrollBar::add-line:horizontal,
QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical
{
    width: 1px;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
    width: 0px;
    height: 0px;
    background: none;
}


QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
    width: 0px;
    height: 0px;
    background: #2A2929;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical
{
    width: 5px;
    height: 5px;
    background: none;
}


QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    width: 0px;
    height: 0px;
    background: #2A2929;
}
/* SCROLL END */

QProgressBar:horizontal
{
    border: 1px solid #3A3939;
    text-align: center;
    padding: 0;
    background: #242526;
    text-align: center;
}

QProgressBar::chunk:horizontal
{
    background-color:  qlineargradient(spread:reflect, x1:0, y1:0, x2:1, y2:0, stop:0 #397EA6, stop:1 #4D8BAF);
}

QToolTip
{
    border: 1px solid transparent;
    background-color: rgb(90, 102, 117);
    color: #f5f5f5;
    padding: 1px;
    opacity: 255;
}

QRadioButton
{
    spacing: 5px;
    outline: none;
    margin-bottom: 2px;
}

QMenuBar
{
    background-color: #414345;
    color: white;
}

QMenuBar::item
{
    background: transparent;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 3px;
    padding-bottom: 3px;
}


QMenuBar::item:selected
{
    background: #5a5b5e;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QMenuBar::item:selected:disabled
